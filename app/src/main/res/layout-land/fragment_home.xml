<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/yess">

    <WebView
        android:id="@+id/Webviehom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </WebView>
<ImageView
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:background="@drawable/cybr"
    android:contentDescription="@string/im">
</ImageView>

    <LinearLayout
        android:id="@+id/ttln"
        android:gravity="center"
        android:layout_marginTop="50dp"
        android:orientation="vertical"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <LinearLayout

            android:gravity="center"
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="110dp"
                android:layout_height="100dp"
                android:id="@+id/usrimg"
                android:contentDescription="@string/im"
                android:background="@drawable/clrpro"
                android:src="@drawable/user">
            </de.hdodenhof.circleimageview.CircleImageView>

            <Button
                android:id="@+id/changee"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/btnimgg"
                tools:ignore="SpeakableTextPresentCheck">

            </Button>

        </LinearLayout>

       <LinearLayout
           android:layout_gravity="center"
           android:gravity="center"
           android:orientation="horizontal"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content">
           <TextView
               android:id="@+id/Nomee"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:textSize="30sp"
               android:textColor="#1FE020">

           </TextView>
           <TextView
               android:id="@+id/disci"
               android:textColor="#1FE020"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:textSize="30sp">

           </TextView>

       </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_marginBottom="2dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/textinfou"
            android:gravity="center"
            android:layout_marginLeft="5dp"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:layout_marginStart="5dp">

        </TextView>
        <ImageView
            android:id="@+id/imgpro"
            android:layout_width="200dp"
            android:layout_height="120dp">

        </ImageView>
    </LinearLayout>


    <com.ebanx.swipebtn.SwipeButton
        android:id="@+id/mdf"
        android:layout_width="160dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:layout_marginBottom="20dp"
        app:button_bottom_padding="5dp"
        app:button_image_disabled="@drawable/ic_baseline_autorenew_25"
        app:button_image_enabled="@drawable/ic_baseline_autorenew_25"
        app:button_left_padding="10dp"
        app:button_right_padding="10dp"
        app:button_top_padding="10dp"
        app:inner_text="Smeskel"
        app:inner_text_background="@drawable/shape_rounded"
        app:inner_text_bottom_padding="15dp"
        app:inner_text_top_padding="15dp">

    </com.ebanx.swipebtn.SwipeButton>
    <TextView
        android:id="@+id/eml"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#720C0707"
        android:textSize="15sp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="350dp">

    </TextView>


    <TextView
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:layout_alignParentBottom="true"
        android:id="@+id/text_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.github.ybq.android.spinkit.SpinKitView
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/progressBar2"
        android:layout_width="100dp"
        android:layout_height="80dp"
        style="@style/SpinKitView.Large.FadingCircle"
        android:layout_gravity="center"
        app:SpinKit_Color="@color/colorAccent" />
</RelativeLayout>